<launch>
    <include file="$(find astra_camera)/launch/astra.launch">
        <arg name="camera" value="cam1" />
        <arg name="device_id" value="$(env CAM1)" />
    </include>
    <include file="$(find lingao_bringup)/launch/robot.launch"/>
    <include file="$(find lingao_navigation)/launch/navigate.launch">
        <arg name="map_file" value="/home/pcms/maps/fambot_map.yaml" />
    </include>
    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find lingao_rviz)/navigation/navigate.rviz"/>
    <node pkg="beginner_tutorials" type="find_prompt.py"  name="find_prompt" />
</launch>
